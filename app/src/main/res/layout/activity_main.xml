<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ceabie.demo.MainActivity">

    <Button
        android:id="@+id/btn_save_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/text"
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:textColor="#f0f0f0"
        android:textSize="18sp"
        android:text="天转到second"
        android:id="@+id/jumpto_lib1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:textColor="#0f0f0f"
        android:text="跳转到split"
        android:id="@+id/jumpto_split"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:textColor="#0f0f0f"
        android:text="跳转到third"
        android:id="@+id/jumpto_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
